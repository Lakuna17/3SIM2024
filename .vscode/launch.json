{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "graalvm",
            "request": "launch",
            "name": "Launch R Script",
            "runtimeExecutable": "Rscript",
            "program": "${file}",
            "debugOptions": {
                "suspend": true
            }
        },
        {
            "type": "graalvm",
            "request": "launch",
            "name": "Launch Python Script",
            "runtimeExecutable": "graalpy",
            "program": "${file}",
            "debugOptions": {
                "suspend": true
            }
        },
        {
            "type": "graalvm",
            "request": "launch",
            "name": "Launch Polyglot App",
            "runtimeExecutable": "polyglot",
            "program": "${file}",
            "debugOptions": {
                "suspend": true
            }
        },
        {
            "type": "graalvm",
            "request": "launch",
            "name": "Launch Node App",
            "outputCapture": "std",
            "program": "${workspaceFolder}/app.js",
            "debugOptions": {
                "suspend": false
            }
        },
        {
            "type": "graalvm",
            "request": "launch",
            "name": "Launch JavaScript",
            "runtimeExecutable": "js",
            "program": "${file}",
            "debugOptions": {
                "suspend": true
            }
        },
        {
            "type": "graalvm",
            "request": "attach",
            "name": "Attach to Remote",
            "protocol": "debugAdapter",
            "address": "TCP/IP address of process to be debugged",
            "port": 4711,
            "localRoot": "",
            "remoteRoot": "Absolute path to the remote directory containing the program"
        },
        {
            "type": "graalvm",
            "request": "attach",
            "name": "Attach",
            "protocol": "debugAdapter",
            "port": 4711
        },
        {
            "name": "Attach to Edge",
            "port": 9222,
            "request": "attach",
            "type": "msedge",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "db2DevExtSqlSpDbg",
            "request": "launch",
            "name": "Db2 Developer Extension Stored Procedure Debug",
            "program": ""
        },
        {
            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "chrome",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        }
    ]
}